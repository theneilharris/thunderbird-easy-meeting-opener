{
  "schemas": [
    {
      "namespace": "LightningMenus",
      "functions": [
        {
          "name": "registerMenus",
          "type": "function",
          "description": "Register calendar context menus",
          "async": true,
          "parameters": []
        }
      ],
      "events": [
        {
          "name": "onCalendarItemClick",
          "type": "function",
          "description": "Fired when a calendar item is clicked",
          "parameters": [
            {
              "name": "info",
              "type": "object",
              "properties": {
                "title": { "type": "string" },
                "startDate": { "type": "string" },
                "endDate": { "type": "string" },
                "description": { "type": "string" }
              }
            }
          ]
        }
      ]
    }
  ]
}